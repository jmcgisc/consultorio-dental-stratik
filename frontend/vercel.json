{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "trailingSlash": false,
  "cleanUrls": true,
 
  "redirects": [
    { "source": "/index.html", "destination": "/", "permanent": true }
  ],
 
  "rewrites": [
    { "source": "/((?!api|.*\\..*).*)", "destination": "/" }
  ],
 
  "headers": [
    {
      "source": "/(images|videos|assets)/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
